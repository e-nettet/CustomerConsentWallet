<div class="page-header">
  <h3>Consents</h3>
</div>

<div class="row">
  <div class="col-sm-12">

    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">Consents for {{email}}</h3>
      </div>

      <div class="panel-body consents">

        {{#if eventError}}
          <div class="alert alert-danger">{{eventError}}</div>
        {{/if}}

        <ul class="list-group">
          {{#each model as |consent|}}
            {{consent-item consent=consent}}
          {{/each}}
        </ul>
      </div>
    </div>
  </div>
</div>
