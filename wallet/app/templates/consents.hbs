<div class="page-header">
  <h1>Consents</h1>
</div>

<div class="row">
  <div class="col-sm-12">

    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">Consents for {{email}}</h3>
      </div>

      <div class="panel-body consents">

        {{#if eventError}}
          <div class="alert alert-danger">{{eventError}}</div>
        {{/if}}

        <ul class="list-group">
          {{#each-in model as |key val|}}            
            {{consent-item consent=val}}
          {{/each-in}}
        </ul>
      </div>
    </div>
  </div>
</div>
